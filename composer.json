{
    "name": "qandidate/toggle-bundle",
    "description": "This Bundle provides the integration with qandidate/toggle. It provides the services and configuration you need to implement feature toggles in your Symfony application.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Willem-Jan",
            "email": "wjzijderveld@gmail.com"
        },
        {
            "name": "Qandidate.com",
            "homepage": "http://labs.qandidate.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Qandidate\\Bundle\\ToggleBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": ">=7.2",
        "qandidate/toggle": "^2.0",
        "symfony/framework-bundle": "^6.0.16||^6.1.8||^6.2.2",
        "symfony/http-foundation": "^6.0.16||^6.1.8||^6.2.2",
        "symfony/http-kernel": "^6.0.16||^6.1.8||^6.2.2",
        "symfony/security-bundle": "^6.0.16||^6.1.8||^6.2.2",
        "doctrine/common":"^2.13||^3.0",
        "doctrine/annotations": "^1.13"
    },
    "require-dev": {
        "symfony/browser-kit": "^6.0.16||^6.1.8||^6.2.2",
        "twig/twig": "^3.0",
        "symfony/twig-bundle": "^6.0.16||^6.1.8||^6.2.2",
        "phpunit/phpunit": "^9.5",
        "matthiasnoback/symfony-dependency-injection-test": "^4.0",
        "matthiasnoback/symfony-config-test": "^4.0",
        "broadway/coding-standard": "^1.2",
        "phpstan/phpstan": "^1.0"
    },
    "suggest": {
        "twig/twig": "For using the twig helper"
    }
}
